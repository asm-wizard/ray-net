mkdir -p build
g++ -c src/main.cpp -o build/main.o
g++ -c include/ray/Ray.cpp -o build/Ray.o
g++ -c include/sdl/SDL_Window.cpp -o build/SDL_Window.o
g++ -c include/misc/Splash.cpp -o build/Splash.o
g++ -Wall -Wextra -pedantic -ggdb build/Splash.o build/Ray.o build/SDL_Window.o build/main.o -o ray-net
./ray-net

